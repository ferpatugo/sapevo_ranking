#tutorial 1 critério adicional
#2 decisores, 3 alternativas e 4 critérios
#sapevo_pesos("Projeto",
#             c("Decisor1","Decisor2"),
#             c("Alternativa1","Alternativa2","Alternativa3"),
#             c("Criterio1","Criterio2","Criterio3","Criterio4"),
#             #vetor_notas_decisores(critérios)
#             c(0,1,2, Comparaçãonova1 , -1,0,3, Comparaçãonova2 , -2,-3,0 , Comparaçãonova3 , Comparaçãonova_-1,Comparaçãonova_-2,Comparaçãonova_-3,0 ),
#             c(0,2,1, Comparaçãonova1 , -2,0,1, Comparaçãonova2 , -1,-1,0 , Comparaçãonova3 , Comparaçãonova_-1,Comparaçãonova_-2,Comparaçãonova_-3,0 )
#             
#             
#)
sapevo_pesos("Projeto",
             c("Decisor1","Decisor2"),
             c("Alternativa1","Alternativa2","Alternativa3"),
             c("Criterio1","Criterio2","Criterio3","Criterio4"),
             #vetor_notas_decisores(critérios)
             c(0,1,2, 1 , -1,0,3, 2 , -2,-3,0 , 3 , -1,-2,-3,0 ),
             c(0,2,1, 1 , -2,0,1, 2 , -1,-1,0 , 3 , -1,-2,-3,0 )
             
             
)

sapevo_ranking("Projeto",
               c("Decisor1","Decisor2"),
               c("Alternativa1","Alternativa2","Alternativa3"),
               c("Criterio1","Criterio2","Criterio3","Criterio4"),
               vetor_peso,
               #vetor_notas_decisores_criterios(alternativas)
               #criterio1
               vetor_notas_decisor1criterio1=c(0,0,-2,  0,0,-1,  2,1,0),
               vetor_notas_decisor2criterio1=c(0,-1,-2,  1,0,-2,  2,2,0),
               
               #criterio2
               vetor_notas_decisor1criterio2=c(0,-3,-2,  3,0,-1,  2,1,0),
               vetor_notas_decisor2criterio2=c(0,-3,-3,  3,0,0,  3,0,0),
               
               #criterio3
               vetor_notas_decisor1criterio3=c(0,-2,-1,  2,0,0, 1,0,0),
               vetor_notas_decisor2criterio3=c(0,-1,-2, 1,0,1,  2,-1,0),
               
               #vetor adicional pela inclusão do Criterio4
               vetor_notas_decisor1criterio4=c(0,-2,-1,  2,0,0, 1,0,0),
               vetor_notas_decisor2criterio4=c(0,-1,-2, 1,0,1,  2,-1,0)
)



